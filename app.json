{
  "expo": {
    "name": "space",
    "slug": "space",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bootsplash_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imisshtml.spaceclimb"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bootsplash_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.imisshtml.spaceclimb"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/bootsplash_logo.png",
          "resizeMode": "contain",
          "imageWidth": 200,
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Pixellari.ttf"
          ]
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c13eed24-e36e-45a6-a491-c44549c34266"
      }
    }
  }
}
